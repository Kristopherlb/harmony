{
    "extends": "../../../tsconfig.base.json",
    "compilerOptions": {
        "jsx": "react-jsx",
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "@golden/*": [
                "../../domains/*/src/index.ts"
            ],
            "@/*": [
                "../../apps/console/client/src/*"
            ]
        }
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}